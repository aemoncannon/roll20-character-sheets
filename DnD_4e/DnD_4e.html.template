		<span class="sheet-spacer"></span>
		
		<div class="sheet-section-core">
		
			<div class="sheet-row">
			
				<div class="sheet-col-1-2 sheet-padr">
					
					<h4 class="sheet-center">Core Stats</h4>
					
					<div class="sheet-row sheet-sub-header">
						<div class="sheet-col-1-6 sheet-center sheet-small-label">Score</div>
						<div class="sheet-col-1-6 sheet-center sheet-small-label">&nbsp;</div>
						<div class="sheet-col-1-6 sheet-center sheet-small-label">Mod</div>
						<div class="sheet-col-1-6 sheet-center sheet-small-label">Mod + 1/2 Level</div>
					</div>

					<!--(for grp in ability_groups)-->
					<div class="sheet-row">
						<div class="sheet-col-1-6">@!in_num(grp.base)!@</div>
						<div class="sheet-col-1-6 sheet-core-stat-label">@!grp.get('label')!@</div>
						<div class="sheet-col-1-6">@!out_num(grp.mod)!@</div>
						<div class="sheet-col-1-6">@!out_num(grp.mod_plus_half_level)!@</div>
						<div class="sheet-col-1-6 sheet-center">
						  <button type="roll" class="sheet-core-save" name="@!grp.get('roll').name!@"
							  value="@!grp.get('roll').value!@">Check</button></div>
					</div>
					<!--(end)-->
					
				</div>
				
				<div class="sheet-col-1-2 sheet-padr">
					
