			<div class="sheet-col-1-4 sheet-small-label sheet-center">@!in_text(character_name, cls='sheet-underlined sheet-center')!@<br/>Character Name</div>
			<div class="sheet-col-1-8 sheet-small-label sheet-center">@!in_text(race, cls='sheet-underlined sheet-center')!@<br/>Race</div>
